/*niven no--no which is divisible by its sum of digits*/

import java.util.Scanner;
class niven
{
	public static void main(String args[])
	{
		Scanner obj=new Scanner(System.in);
		int n,sum=0,rem=0,temp;
		System.out.println("enter any no.");
		n=obj.nextInt();
		temp=n;
		while(n>0)
		{
			rem=n%10;
			sum=sum+rem;
			n=n/10;
		}
		if(temp%sum==0)
			System.out.println("no is niven");
		else
			System.out.println("not a niven");
		
	}
}
			
			
			
		
